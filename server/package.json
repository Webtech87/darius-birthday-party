{
  "name": "darius-birthday-party",
  "version": "1.0.0",
  "description": "Birthday party RSVP application with React frontend and Flask backend",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && python run.py",
    "client:dev": "cd client && npm run dev",
    "server:start": "cd server && python app.py",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "install:client": "cd client && npm install",
    "build": "npm run client:build",
    "start": "npm run server:start"
  },
  "keywords": ["birthday", "party", "rsvp", "react", "flask", "python"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}